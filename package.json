{
  "name": "mova",
  "version": "0.9.0-rc.1",
  "description": "No-Code Automation Platform for MOVA",
  "type": "module",
  "scripts": {
    "build:keys": "node scripts/build/build_keys.mjs",
    "forms:lint": "node scripts/forms/validate_form_spec.mjs templates/forms && node scripts/forms/lint_bindings.mjs templates/forms",
    "forms:build:dev": "node scripts/forms/build_canonical.mjs templates/forms --mode=dev --fill-sample",
    "forms:build:prod": "node scripts/forms/build_canonical.mjs templates/forms --mode=prod",
    "validate:canonical": "node scripts/validation/validate_canonical.mjs",
    "validate:schemas": "node scripts/validation/validate_schemas.mjs",
    "smoke": "node scripts/run/smoke.mjs templates/canonical",
    "build:all": "npm run forms:lint && npm run forms:build:dev && npm run validate:canonical && npm run smoke",
    "build": "npm run build:all",
    "release:check": "npm run forms:lint && npm run forms:build:prod && npm run validate:canonical && npm run smoke",
    "start:ui": "node server.js",
    "run": "node scripts/runtime/run_plan.mjs",
    "cli": "node cli.mjs",
    "bench": "node bench/bench.mjs",
    "sbom": "npx @cyclonedx/cyclonedx-npm --output-file sbom/bom.json",
    "test": "npm run validate:schemas"
  },
  "engines": {
    "node": ">=18.20 <=22.x"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "serve": "^14.2.5"
  }
}
