{
  "mova_version": "3.3",
  "actions": [
    {
      "type": "http_request",
      "invoke": "http:request",
      "payload": {
        "url": "https://api.telegram.org/bot{env.TELEGRAM_BOT_TOKEN}/sendMessage",
        "method": "POST",
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "chat_id": "123456",
          "text": "Готуємось до демо"
        },
        "result_in": "vars.telegram"
      }
    }
  ]
}
