{
  "mova_version": "3.3",
  "actions": [
    {
      "type": "http_request",
      "invoke": "http:request",
      "payload": {
        "url": "https://api.notion.com/v1/pages",
        "method": "POST",
        "headers": {
          "Authorization": "Bearer {env.NOTION_TOKEN}",
          "Content-Type": "application/json",
          "Notion-Version": "2022-06-28"
        },
        "body": {
          "parent": {
            "database_id": "tasks"
          },
          "properties": {
            "Name": {
              "title": [
                {
                  "text": {
                    "content": "Підготувати реліз"
                  }
                }
              ]
            }
          }
        }
      }
    }
  ]
}
