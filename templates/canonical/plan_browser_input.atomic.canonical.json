{
  "mova_version": "3.3.0",
  "id": "mova.browser_input.atomic",
  "description": "Atomic call for mova.browser_input",
  "policies": {
    "timeout_ms": 10000,
    "retry": {
      "max_attempts": 1
    }
  },
  "actions": [
    {
      "type": "emit_event",
      "channel": "mova.browser_input",
      "payload": {
        "text": "${text}",
        "press_enter": "${press_enter}"
      }
    }
  ]
}