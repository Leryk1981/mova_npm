{
  "mova_version": "3.3",
  "id": "mova.file_read.atomic",
  "description": "Atomic call for mova.file_read",
  "policies": {
    "timeout_ms": 10000,
    "retry": {
      "max_attempts": 1
    }
  },
  "actions": [
    {
      "type": "emit_event",
      "channel": "mova.file_read",
      "payload": {
        "path": "${path}"
      }
    }
  ]
}