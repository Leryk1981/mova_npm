{
  "version": "3.3",
  "nutzlast_typ": "application/ld+json",
  "$comment": "JSON-LD LocalBusiness: Basis-SEO-Profil f√ºr lokale Unternehmen.",
  "nutzlast": {
    "@context": {
      "@vocab": "https://schema.org/",
      "@language": "de"
    },
    "@type": "LocalBusiness",
    "name": "<BUSINESS_NAME>",
    "url": "<URL>",
    "logo": "<LOGO_URL>",
    "address": {
      "@type": "PostalAddress",
      "streetAddress": "<STREET_ADDRESS>",
      "addressLocality": "<ADDRESS_LOCALITY>",
      "addressRegion": "<ADDRESS_REGION>",
      "postalCode": "<POSTAL_CODE>",
      "addressCountry": "<ADDRESS_COUNTRY>"
    },
    "telephone": "<TELEPHONE>",
    "openingHours": [
      "<OPENING_HOURS_ARRAY>"
    ]
  },
  "aktionen": [
    {
      "typ": "datei_schreiben",
      "pfad": "<OUT_FILE>",
      "wert": "${payload}",
      "content_type": "application/ld+json",
      "$comment": "Oder basierend auf dem root payload_type; zur Klarheit dupliziert."
    },
    {
      "typ": "drucken",
      "nachricht": "JSON-LD LocalBusiness in <OUT_FILE> gespeichert"
    }
  ]
}