{
  "wersja": "3.3",
  "$comment": "Discord: wysłać wiadomość przez Incoming Webhook.",
  "akcje": [
    {
      "typ": "pobrać_sekret",
      "name": "<DISCORD_WEBHOOK_SECRET_NAME>",
      "out": "discord_webhook_url"
    },
    {
      "typ": "żądanie_http",
      "metoda": "POST",
      "url": "{+discord_webhook_url}",
      "nagłówki": {
        "Content-Type": "application/json"
      },
      "ciało": {
        "content": "<CONTENT>",
        "username": "<USERNAME>",
        "avatar_url": "<AVATAR_URL>"
      },
      "out": "vars.resp"
    },
    {
      "typ": "drukować",
      "wiadomość": "Discord zaakceptował (status w vars.resp.__status)"
    }
  ]
}
