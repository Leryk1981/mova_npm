{
  "mova_version": "3.3.0",
  "id": "mova.file_write.atomic",
  "description": "Atomic call for mova.file_write",
  "policies": {
    "timeout_ms": 10000,
    "retry": {
      "max_attempts": 1
    }
  },
  "actions": [
    {
      "type": "emit_event",
      "channel": "mova.file_write",
      "payload": {
        "path": "${path}",
        "content": "${content}"
      }
    }
  ]
}