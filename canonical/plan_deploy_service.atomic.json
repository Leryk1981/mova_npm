{
  "mova_version": "3.3",
  "id": "mova.deploy_service.atomic",
  "description": "Atomic call for mova.deploy_service",
  "policies": {
    "timeout_ms": 10000,
    "retry": {
      "max_attempts": 1
    }
  },
  "actions": [
    {
      "type": "emit_event",
      "channel": "mova.deploy_service",
      "payload": {
        "service_name": "${service_name}"
      }
    }
  ]
}