{
  "mova_version": "3.3.0",
  "id": "mova.browser_click.atomic",
  "description": "Atomic call for mova.browser_click",
  "policies": {
    "timeout_ms": 10000,
    "retry": {
      "max_attempts": 1
    }
  },
  "actions": [
    {
      "type": "emit_event",
      "channel": "mova.browser_click",
      "payload": {
        "index": "${index}"
      }
    }
  ]
}