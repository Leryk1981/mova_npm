{
  "mova_version": "3.3",
  "actions": [
    {
      "type": "log",
      "invoke": "console:log",
      "payload": {
        "message": "Починаємо..."
      }
    },
    {
      "type": "delay",
      "invoke": "flow:delay",
      "payload": {
        "ms": 2000
      }
    },
    {
      "type": "log",
      "invoke": "console:log",
      "payload": {
        "message": "Завершено після 2-секундної затримки."
      }
    }
  ]
}